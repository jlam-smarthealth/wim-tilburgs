# Environment variables template voor wimtilburgs.nl
# Copy this file to .env and fill in your actual values

# Application Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_API_VERSION=v1

# Database Configuration (Development)
DATABASE_URL=postgresql://user:password@localhost:5432/wimtilburgs_dev
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=wimtilburgs_dev
DATABASE_USER=your_username
DATABASE_PASSWORD=your_password

# AI Services Configuration
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_key_here
ANTHROPIC_API_KEY=your_claude_api_key_here

# GPT-5 Configuration
GPT5_API_URL=https://api.openai.com/v1
GPT5_MODEL=gpt-5
GPT5_MAX_TOKENS=4000

# Translation Services
GOOGLE_TRANSLATE_API_KEY=your_translate_key_here
DEEPL_API_KEY=your_deepl_key_here

# Azure Services
AZURE_STORAGE_CONNECTION_STRING=your_azure_storage_connection
AZURE_BLOB_CONTAINER=wimtilburgs-assets

# Analytics & Monitoring
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
MICROSOFT_CLARITY_ID=your_clarity_id

# Security
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Email Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Social Media Integration
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_secret

# Feature Flags
FEATURE_GPT5_INTEGRATION=true
FEATURE_MULTILINGUAL_BLOG=true
FEATURE_AI_MOTOR_LAB=true
FEATURE_PREMIUM_MOTORS=false

# Development Settings
DEBUG=false
LOG_LEVEL=info
PORT=3000